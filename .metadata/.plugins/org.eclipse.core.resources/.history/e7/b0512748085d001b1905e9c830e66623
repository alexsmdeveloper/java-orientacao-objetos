
public class TesteConexao {

	public static void main(String[] args) {

		System.out.println("inicio main...");
		
		Conexao con = null;

		try {
			con = new Conexao();
			con.leDados();
			con.fecha();
		} catch (IllegalStateException e) {
			System.out.println("Erro de conexao>" + e.getMessage());
			e.printStackTrace();
		} finally {
			con.fecha();
		}

		System.out.println("fim main.");
	}

}
