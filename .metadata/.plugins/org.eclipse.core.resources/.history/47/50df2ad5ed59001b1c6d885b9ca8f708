
public class CriaConta {
	
	public static void main(String[] args) {
		
		Conta contaDoAlex = new Conta();
		
		//Cliente clienteAlex = new Cliente(); 
		//contaDoAlex.titular = clienteAlex; ou
		
		contaDoAlex.titular = new Cliente(); //forma direta
		contaDoAlex.titular.nome = "Alex"; //definindo o nome do Cliente através da referência conta!
		
		System.out.println(contaDoAlex.titular.nome);
	
		
	}
}
