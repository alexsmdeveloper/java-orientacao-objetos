
public class Conta {

	void deposita() {
	}
	
	void Acessa() {
		System.out.println("Conta Acessada com sucesso!");
	}

	void emprestimoAlemDoLimite() throws MinhaExcecaoChecked {
	}

	void liberaCartaoCredito(int tipo) throws MinhaExcecaoChecked {
		switch (tipo) {
		case 1:
			System.out.println("teste do lançamento nº 1");
			break;
		case 2:
			System.out.println("teste do lançamento nº 2");
			break;
		default:
			throw new MinhaExcecaoChecked("Cartão de Crédito não liberado!");

		}
	}

}
